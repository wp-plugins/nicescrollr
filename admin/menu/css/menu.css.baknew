.wrap h2.nsr-page-title:not(.nav-tab-wrapper) {
    font-family: 'fugaz_one_regular', 'Open Sans', sans-serif;
    font-size: 36px;
    font-weight: 500;
    box-sizing: border-box;
    width: 100%;
    max-width: 480px;
    padding: 50px 40px 30px 12px;
    margin: 0;
    color: #555;
    text-shadow:
        1px 0 1px rgb(204, 204, 204),
        0 1px 1px rgb(238, 238, 238),
        2px 1px 1px rgb(204, 204, 204),
        1px 2px 1px rgb(238, 238, 238),
        3px 2px 1px rgb(204, 204, 204);
}

.nsr-page-subtitle {
    font-family: 'fugaz_one_regular', 'Open Sans', sans-serif;
    font-size: 14px;
    font-weight: 400;
    box-sizing: border-box;
    width: 100%;
    max-width: 480px;
    padding: 0 20px 40px 12px;
    margin: 0;
    color: #666;
    text-shadow:
        1px 0 1px rgb(204, 204, 204),
        0 1px 1px rgb(238, 238, 238);
}

h2.nav-tab-wrapper {
    box-sizing: border-box;
    width: 100%;
    max-width: 480px;
    padding: 30px 0 0 12px;
    margin: 0;
}

h2.nav-tab-wrapper, h3.nav-tab-wrapper {
    border-bottom: none;
}

.wrap  h2 a.nav-tab {
    font-family: 'Open Sans', sans-serif;
    font-weight: 500;
    color: #0a1318;
    padding-bottom: 8px;
}

.nav-tab-active {
    color: #0a1318;
    background-color: #f8f8f8;
    transition: 0.2s all;
    z-index: 1;
}

.nav-tab-active:hover {
    background-color: #fff;
    transition: 0.2s all;
}

.wrap h2 a.nav-tab:before {
    color: #24465a;
    font-family: 'icomoon', sans-serif;
    font-size: 16px;
    margin-right: 5px;
    right: 8px;
    text-shadow: -0.6px -0.6px 0 rgba(241, 241, 241, 0.8), 1px 1px 1px #5c5c5c;
}

/**************************************************************
 * Settings Headings
 *************************************************************/
.nicescrollr_settings_toggle {
    font-family: 'Open Sans', sans-serif;
    font-weight: 500;
    font-size: 20px;
    box-sizing: border-box;
    width: 100%;
    max-width: 480px;
    padding: 26px 0 30px 12px;
    margin-top: 0;
    margin-bottom: 0;
    border: 1px solid rgb(229, 229, 229);
    background: none repeat scroll 0% 0% rgb(255, 255, 255);
    -webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.1), 0 0 40px rgba(0, 0, 0, 0.06) inset;
    -moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.1), 0 0 40px rgba(0, 0, 0, 0.06) inset;
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.1), 0 0 40px rgba(0, 0, 0, 0.06) inset;
    transition: all 0.2s ease-in-out;
}

.nicescrollr_settings_toggle:before {
    color: #24465a;
    font-family: 'icomoon', sans-serif;
    font-size: 24px;
    margin-right: 4px;
    position: relative;
    top: 3px;
    right: 4px;
    text-shadow: -1px -1px 0 rgba(241, 241, 241, 0.8), 1px 1px 1px #5c5c5c;
}

.nicescrollr_settings_toggle:before,
.nicescrollr_settings_toggle:after {
    -webkit-box-shadow: 0 0 12px rgba(0, 0, 0, 0.09);
    -moz-box-shadow: 0 0 12px rgba(0, 0, 0, 0.09);
    box-shadow: 0 0 12px rgba(0, 0, 0, 0.09);
    -moz-border-radius: 100px / 10px;
    border-radius: 100px / 10px;
}

.nicescrollr_settings_toggle:after {
    -webkit-transform: skew(8deg) rotate(3deg);
    -moz-transform: skew(8deg) rotate(3deg);
    -ms-transform: skew(8deg) rotate(3deg);
    -o-transform: skew(8deg) rotate(3deg);
    transform: skew(8deg) rotate(3deg);
}

.nicescrollr_settings_toggle:hover {
    border: 1px solid rgb(229, 229, 229);
    background: none repeat scroll 0% 0% rgb(255, 255, 255);
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    cursor: pointer;
}

/**************************************************************
 * Options Table
 *************************************************************/
.form-table {
    margin-top: 3px;
}

form#nsr_form,
form#nsr_form table,
.form-table-wrap,
.form-table-wrap tbody,
.form-table-wrap tbody tr {
    box-sizing: border-box;
    width: 100%;
    max-width: 480px;
}

#nsr_form table {
    width: inherit;
    table-layout: fixed;
    box-sizing: content-box;
}

#nsr_form tbody {
    width: 100%;
    min-width: 100%;
    display: table;
}

#nsr_form tr {
    box-sizing: border-box;
    display: table;
    width: 100%;
    border: 1px solid rgb(229, 229, 229);
    height: 48px;
    background: none repeat scroll 0% 0% rgb(255, 255, 255);
    clear: both;
    border-radius: 2px;
    -webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.03), 0 0 40px rgba(0, 0, 0, 0.01) inset;
    -moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.03), 0 0 40px rgba(0, 0, 0, 0.01) inset;
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.03), 0 0 40px rgba(0, 0, 0, 0.01) inset;
}

#nsr_form tr:before,
#nsr_form tr:after {
    -webkit-box-shadow: 0 0 12px rgba(0, 0, 0, 0.03);
    -moz-box-shadow: 0 0 12px rgba(0, 0, 0, 0.03);
    box-shadow: 0 0 12px rgba(0, 0, 0, 0.03);
    -moz-border-radius: 100px / 10px;
    border-radius: 100px / 10px;
}

#nsr_form tr:after {
    -webkit-transform: skew(8deg) rotate(3deg);
    -moz-transform: skew(8deg) rotate(3deg);
    -ms-transform: skew(8deg) rotate(3deg);
    -o-transform: skew(8deg) rotate(3deg);
    transform: skew(8deg) rotate(3deg);
}

input[type="text"].nsr-switch {
    width: 100px !important;
}

/* Input field heading */
.form-table th {
    box-sizing: border-box;
    font-family: 'Open Sans', sans-serif;
    font-size: 14px;
    font-weight: 400;
    width: 200px;
    height: 40px;
    padding: 0;
    padding-top: 17px;
    padding-left: 16px;
    float: left;
}

/* Input field and description */
.form-table td {
    box-sizing: border-box;
    font-family: 'Open Sans', sans-serif;
    height: 40px;
    padding: 0;
    padding-top: 7px;
    min-width: 200px;
    max-width: 100%;
    float: left;
}

.form-table td p {
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 16px;
}

/* Input field container */
.form-table td p:first-of-type {
    display: inline-block;
    width: 160px;
    float: left;
}

/* Description Container ("Label") */
.form-table td p:last-of-type {
    display: inline-block;
    min-width: 240px;
    position: relative;
    top: 4px;
    float: left;
}

.form-table-td-wrap {
    clear: both;
}

@media screen and (max-width: 780px) {

    .nsr-page-title {
        padding: 30px 0 30px 12px;
    }

    .form-table th {
        display: block;
        width: 100%;
        height: auto;
        padding: 12px 0 0 24px;
        margin: 0;
        float: none;
    }

    .form-table td {
        display: block;
        width: 100%;
        height: auto;
        padding: 12px 0 0 24px;
        margin: 0;
        float: none;
    }

    .form-table td p {
        display: block;
        width: 100%;
        height: auto;
        padding: 0;
        margin: 0;
        float: none;
    }

    .form-table label {
        padding: 18px 0 0 24px;
    }

    .form-table td p:first-of-type {
        width: 160px;
        display: inline-block;
        margin-bottom: 12px;
        float: left;
    }

    .form-table td p:last-of-type {
        width: auto;
        display: inline-block;
        float: left;
    }

    .form-table-td-wrap {
        height: auto;
        clear: both;
    }

}

@media screen and (max-width: 480px) {

    .nsr-page-title {
        padding: 20px 0 20px 12px;
    }

    #nsr_form tr {
        height: auto;
    }

    .form-table th {
        box-sizing: border-box;
        display: block;
        width: 100%;
        height: auto;
        padding: 12px 0 0 24px;
        margin: 0;
        float: none;
    }

    .form-table td {
        box-sizing: border-box;
        display: block;
        width: 100%;
        height: auto;
        padding: 12px 0 0 24px;
        margin: 0;
        float: none;
    }

    .form-table td input[type="text"],
    .form-table td select {
        width: 70%;
    }

    .form-table td p {
        box-sizing: border-box;
        display: block;
        width: 100%;
        height: auto;
        padding: 0;
        margin: 0;
        float: none;
    }

    .form-table label {
        box-sizing: border-box;
        margin: 12px 0 12px 0;
        padding: 12px 0 12px 0;
    }

    /* .nicescroll-input-container */
    .form-table td p:first-of-type {
        box-sizing: border-box;
        width: 100%;
        display: block;
        margin-bottom: 0;
        float: none;
    }

    /* .nicescroll-label-container */
    .form-table td p:last-of-type {
        box-sizing: border-box;
        width: 100%;
        display: block;
        padding: 12px 0 12px 0;
        float: none;
    }

    .form-table-td-wrap {
        box-sizing: border-box;
        height: auto;
        clear: none;
    }

}

